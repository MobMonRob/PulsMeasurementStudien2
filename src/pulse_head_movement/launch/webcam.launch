<?xml version="1.0"?>
<launch>
    <arg name="bdf_file" default="None" />
    <arg name="show_image_frame" default="false" />
    <arg name="fps" default="61" />
    <arg name="video_file" default="0"/>
    <include file="$(find face_detection)/launch/webcam.launch">
        <arg name="bdf_file" default="$(arg bdf_file)" />
        <arg name="show_image_frame" default="$(arg show_image_frame)" />
        <arg name="fps" default="$(arg fps)" />
        <arg name="video_file" default="$(arg video_file)"/>
    </include>
    <include file="$(find pulse_head_movement)/launch/pulse_head_movement.launch">
        <arg name="topic" value="/face_detection/mask" />
    </include>
</launch>
